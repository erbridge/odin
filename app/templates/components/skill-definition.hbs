<div class='name'><label>name: {{input type='text' value=skill.name}}</label></div>

<div class='type'><label>type:

  {{#x-select value=skill.type}}

    {{#x-option}}not set{{/x-option}}

    {{#each skill.typeNames as |typeName type|}}

      {{#x-option value=type}}{{typeName}}{{/x-option}}

    {{/each}}

  {{/x-select}}

</label></div>
